<configuration>
  <system.webServer>
    <staticContent>
      <!-- Unity 5.x -->
      <mimeMap fileExtension=".data" mimeType="application/binary" />
      <mimeMap fileExtension=".mem" mimeType="application/binary" />

      <!-- Unity 5.3 -->
      <mimeMap fileExtension=".datagz" mimeType="application/binary" />
      <mimeMap fileExtension=".jsgz" mimeType="application/binary" />
      <mimeMap fileExtension=".memgz" mimeType="application/binary" />
    </staticContent>
  </system.webServer>
  
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <pages theme="Muted_Gradient">
      <controls>
        <!--<add tagPrefix="UserControl" tagName="WizardControl" src="/Controls/Contact_UserControl.ascx" />-->
      </controls>
    </pages>
  </system.web>
  
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
  
</configuration>
